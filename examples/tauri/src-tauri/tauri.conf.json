{
  "build": {
    "beforeDevCommand": "pnpm --filter @rspc/examples-astro -- dev",
    "beforeBuildCommand": "pnpm --filter @rspc/examples-astro -- build",
    "devPath": "http://localhost:3000",
    "distDir": "../../astro/dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "rspc-tauri-example",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "rspc-tauri-example",
        "width": 800,
        "height": 600
      }
    ]
  }
}
